{
  "name": "episode-vuerouter-team",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "config": {
    "content_base": "target/"
  },
  "scripts": {
    "list-installed": "yarn run _make list:installed",
    "patch-apply": "yarn run _make patch:apply",
    "make-install": "yarn add shelljs@^0.7.7 -s && yarn run _make",
    "make-help": "./makejs --help",
    "postinstall": "cd node_modules/.bin && ln -s ../webpack-dev-server/bin/webpack-dev-server.js webpack-dev-server",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --hot --open --content-base $npm_package_config_content_base",
    "lint": "eslint --ext .js,.vue src/* test/*",
    "_make": "./makejs",
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "6",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "laravel-mix": "^0.11.3",
    "shelljs": "^0.7.7",
    "vue-loader": "^12.0.2",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.0"
  },
  "dependencies": {
    "vue": "^2.3.0",
    "vue-router": "^2.5.2",
    "vuex": "^2.3.1"
  },
  "engines": {
    "node": ">= 4.0.0",
    "yarn": ">= 0.22.0"
  }
}
