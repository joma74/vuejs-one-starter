{
  "name": "episode-vuerouter-team",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "config": {
    "content_base": "target/",
    "allure_results_base": "target/allure-results",
    "chromedriverDownloadUrl": "https://chromedriver.storage.googleapis.com/2.29/chromedriver_linux64.zip",
    "firefoxdriverDownloadUrl": "https://github.com/mozilla/geckodriver/releases/download/v0.16.1/geckodriver-v0.16.1-linux64.tar.gz",
    "libs_dir": "libs/"
  },
  "scripts": {
    "list-installed": "./makejs list:installed",
    "patch-apply": "./makejs patch:apply",
    "link-bin": "./makejs link:bin",
    "make-clean-install": "./makejs install:clean_install:all",
    "make-help": "./makejs --help",
    "postinstall": "./makejs install:postinstall",
    "check-lint": "./makejs check:lint",
    "pretest": "./makejs test:clean",
    "test": "./makejs test --",
    "makejs": "./makejs",
    "test-debug": "node $NODE_DEBUG_OPTION ./makejs test:debug -- ",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --hot --https --content-base $npm_package_config_content_base",
    "bundle-stats": "cross-env NODE_ENV=production webpack --progress --display-modules --display-chunks --profile --json > $npm_package_config_content_base/stats.json",
    "bundle-report": "webpack-bundle-analyzer $npm_package_config_content_base/stats.json",
    "test-report": "allure serve $npm_package_config_allure_results_base",
    "serve-app": "cross-env NODE_ENV=production webpack-dev-server --https --progress --compress --content-base $npm_package_config_content_base"
  },
  "devDependencies": {
    "allure-commandline": "^2.0.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "6",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^3.5.0",
    "chai-string": "^1.3.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "debug": "^2.6.8",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^2.0.1",
    "fakerest": "^2.0.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "laravel-mix": "^0.11.3",
    "mocha": "^3.4.2",
    "mocha-allure-reporter": "^1.3.1",
    "mocha-multi-reporters": "^1.1.4",
    "selenium-webdriver": "^3.4.0",
    "shelljs": "^0.7.7",
    "sinon": "^2.1.0",
    "targz": "^1.0.1",
    "unzipper": "^0.8.8",
    "use-config": "^1.0.2",
    "vue-loader": "^12.0.2",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.0",
    "webpack-bundle-analyzer": "^2.6.0",
    "wget-improved": "^1.4.0"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "request": "^2.81.0",
    "vue": "^2.3.0",
    "vue-inject": "^1.0.0",
    "vue-router": "^2.5.2",
    "vuex": "^2.3.0"
  },
  "engines": {
    "node": ">= 7.9.0",
    "yarn": ">= 0.24.5"
  }
}
